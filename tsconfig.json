    "target": "es2022",
    "module": "esnext",
    "lib": ["es2022"],
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true
